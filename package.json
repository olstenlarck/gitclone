{
  "name": "gitclone",
  "version": "1.0.3",
  "description": "Clone a Github repository with only `username/repo`. Support CLI.",
  "main": "index.js",
  "scripts": {
    "clean": "rm -rf node_modules week-seconds",
    "lint": "npm run clean && npm install && jshint bin/*.js lib/*.js test/*.js index.js",
    "test": "npm run lint && node test/test.js"
  },
  "author": {
    "name": "Charlike Make Reagent",
    "email": "mameto_100@mail.bg",
    "url": "https://github.com/tunnckoCore",
    "twitter": "https://twitter.com/tunnckoCore"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tunnckoCore/gitclone.git"
  },
  "keywords": [
    "git",
    "cli",
    "support",
    "helper",
    "github",
    "utils",
    "utility",
    "util",
    "clone",
    "repo",
    "repository"
  ],
  "dependencies": {
    "minimist": "*",
    "native-or-another": "*",
    "sliced": "*"
  },
  "devDependencies": {
    "micropromise": "*",
    "jshint": "*"
  },
  "licenses": [{
    "type": "MIT",
    "url": "https://github.com/tunnckoCore/gitclone/blob/master/license.md"
  }],
  "engines": {
    "node": ">= 0.10.0",
    "npm": ">= 1.4.28"
  },
  "bin": {
    "gitclone": "bin/cli.js"
  },
  "preferGlobal": "true"
}
